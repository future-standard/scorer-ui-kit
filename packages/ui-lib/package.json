{
  "name": "scorer-ui-kit",
  "version": "2.0.0",
  "description": "SCORER UI Components",
  "author": "Josh Lipps",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/future-standard/scorer-ui-kit.git"
  },
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.tsx",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "microbundle-crl --no-compress --format modern,cjs",
    "start": "microbundle-crl watch --no-compress --format modern,cjs",
    "prepublishOnly": "run-s build",
    "test": "run-s test:unit test:lint test:build",
    "test:build": "run-s build",
    "test:lint": "eslint ./src --ext .ts,.tsx",
    "test:unit": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "svgr": "svgr --typescript -d src/svg/ src/svg",
    "predeploy": "cd ../example && npm run build && cd ../storybook && npm run build-storybook",
    "deploy": "gh-pages -d ../example/build",
    "actions-deploy": "gh-pages -d ../example/build",
    "link": "npm link && npm link react && npm link react-dom && npm link react-router-dom && npm link styled-components",
    "zeplin-connect": "zeplin connect"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router": "^5.3.1",
    "react-router-dom": "^5.2.0",
    "react-use-websocket": "^2.9.1",
    "styled-components": "^5.3.5"
  },
  "devDependencies": {
    "@svgr/cli": "^6.3.1",
    "@types/jest": "^28.1.6",
    "@types/lodash.debounce": "^4.0.7",
    "@types/lodash.defaultsdeep": "^4.6.7",
    "@types/lodash.isequal": "^4.5.6",
    "@types/node": "^16.11.7",
    "@types/react": "^17.0.34",
    "@types/react-router-dom": "^5.1.5",
    "@types/styled-components": "^5.1.25",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "@zeplin/cli": "^1.1.12",
    "@zeplin/cli-connect-react-plugin": "^1.1.1",
    "@zeplin/cli-connect-storybook-plugin": "^0.5.4",
    "babel-eslint": "^10.0.3",
    "cross-env": "^7.0.3",
    "eslint": "^7.12.1",
    "eslint-config-standard-react": "^11.0.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-standard": "^4.1.0",
    "gh-pages": "^4.0.0",
    "microbundle-crl": "^0.13.11",
    "npm-run-all": "^4.1.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^5.0.1",
    "react-use-websocket": "^2.9.1",
    "styled-components": "^5.3.5"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@future-standard/icons": "^2.0.0",
    "@types/react-dom": "^17.0.0",
    "date-fns": "^2.29.1",
    "immutability-helper": "^3.1.1",
    "lodash.debounce": "^4.0.7",
    "lodash.defaultsdeep": "^4.6.1",
    "lodash.isequal": "^4.5.0"
  },
  "directories": {
    "example": "example"
  },
  "bugs": {
    "url": "https://github.com/future-standard/scorer-ui-kit/issues"
  },
  "homepage": "https://future-standard.github.io/scorer-ui-kit/#/"
}
